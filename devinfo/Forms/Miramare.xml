            <instance>
                <nm id="Miramare">
                    <name />
                    <nationality />
                    <temp />
                    <humidity />
                    <wind />
                    <repeat_observation jr:template="">
                        <habitat />
                        <observe />
                        <see />
                        <bird />
                        <rept_amph />
                        <rodents />
                        <shrews />
                        <carnivores />
                        <ungulates />
                        <marinemammals />
                        <sign />
                        <tracks_clear />
                        <tracks_length />
                        <tracks_width />
                        <tracks_stride_length />
                        <tracks_stride_width />
                        <location/>
                        <image/>
                        <notes/>
                    </repeat_observation>
                </nm>
            </instance>
        
            <bind nodeset="/nm/name" type="string" />
            <bind nodeset="/nm/nationality" type="string" />
            <bind nodeset="/nm/temp" type="integer"/>
            <bind nodeset="/nm/humidity" type="select1" />
            <bind nodeset="/nm/wind" type="select1" />
            
            <bind nodeset="/nm/repeat_observation/habitat" type="select1"/>
            <bind nodeset="/nm/repeat_observation/observe" type="select1"/> 
            <bind nodeset="/nm/repeat_observation/see" type="select1" relevant="selected(/nm/repeat_observation/observe, 'see')"/>
            <bind nodeset="/nm/repeat_observation/sign" type="select1" relevant="selected(/nm/repeat_observation/observe, 'signs')"/> 
            
            <bind nodeset="/nm/repeat_observation/bird" type="select1" relevant="selected(/nm/repeat_observation/see, 'bird')"/>       
            <bind nodeset="/nm/repeat_observation/rept_amph" type="select1" relevant="selected(/nm/repeat_observation/see, 'rept_amph')"/> 
            <bind nodeset="/nm/repeat_observation/rodents" type="select1" relevant="selected(/nm/repeat_observation/see, 'rodents')"/> 
            <bind nodeset="/nm/repeat_observation/shrews" type="select1" relevant="selected(/nm/repeat_observation/see, 'shrews')"/> 
            <bind nodeset="/nm/repeat_observation/carnivores" type="select1" relevant="selected(/nm/repeat_observation/see, 'carnivores')"/>
            <bind nodeset="/nm/repeat_observation/ungulates" type="select1" relevant="selected(/nm/repeat_observation/see, 'ungulates')"/> 
            <bind nodeset="/nm/repeat_observation/marinemammals" type="select1" relevant="selected(/nm/repeat_observation/see, 'marinemammals')"/>
            
            <bind nodeset="/nm/repeat_observation/tracks_clear" type="select1" relevant="selected(/nm/repeat_observation/sign, 'tracks')"/>
            <bind nodeset="/nm/repeat_observation/tracks_length" type="integer" relevant="selected(/nm/repeat_observation/sign, 'tracks')"/>
            <bind nodeset="/nm/repeat_observation/tracks_width" type="integer" relevant="selected(/nm/repeat_observation/sign, 'tracks')"/>
            <bind nodeset="/nm/repeat_observation/tracks_stride_length" type="integer" relevant="selected(/nm/repeat_observation/sign, 'tracks')"/>
            <bind nodeset="/nm/repeat_observation/tracks_stride_width" type="integer" relevant="selected(/nm/repeat_observation/sign, 'tracks')"/>
            
            <bind nodeset="/nm/repeat_observation/location" type="geopoint"/>
            <bind nodeset="/nm/repeat_observation/image" type="binary"/>
            <bind nodeset="/nm/repeat_observation/notes" type="string"/>
              
        </model>
    </h:head>
    <h:body>
        <group appearance="field-list">
            <label>Demographic information</label>
            <input ref="name">
                <label>Please enter your name(s):</label>
            </input>
            <input ref="nationality">
                <label>Please enter your country (or countries) of origin:</label>
            </input>
        </group>
        <group appearance="field-list">
            <label>Weather information</label>
            <input ref="temp">
                <label>Temperature:</label>
            </input>
            <select1 ref="humidity">
                <label>Humidity:</label>
                <item><label>Dry or low</label><value>low</value></item>
                <item><label>Normal or medium</label><value>med</value></item>
                <item><label>Wet or High</label><value>high</value></item>
            </select1>
            <select1 ref="wind">
                <label>Wind conditions:</label>
                <item><label>Little or no wind</label><value>low</value></item>
                <item><label>Breezy or light wind</label><value>med</value></item>
                <item><label>Strong or very windy</label><value>high</value></item>
            </select1>
       </group>
       <group>
