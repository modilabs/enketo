      <instance>
        <NewWidgets id="NewWidgets">
          <numberAsString/>
          <locationMap/>
          <time/>
          <dateTime/>
          <q1/>
          <q2/>
          <q0/>
          <q3/>
          <q4/>
          <q4a/>
          <q5/>
          <q6/>
          <q7/>
          <listGroup>
            <q8/>
            <q9/>
            <q10/>
            <q11/>
            <q12/>
          </listGroup>
          <multiListGroup>
            <q13/>
            <q14/>
            <q15/>
          </multiListGroup>
          <repeatingFieldList jr:template="">
            <repeat_string/>
            <repeat_int>123456789</repeat_int>
          </repeatingFieldList>
        </NewWidgets>
      </instance>
      <bind nodeset="/NewWidgets/numberAsString" type="string"/>
